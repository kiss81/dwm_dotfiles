#BrightnessUp
"xbacklight +10"
  m:0x0 + c:233
  XF86MonBrightnessUp

BrightnessDown
"xbacklight -10"
  m:0x0 + c:232
  XF86MonBrightnessDown

# volume control
"amixer sset Master 5%+ unmute"
    m:0x0 + c:123
    XF86AudioRaiseVolume

"amixer sset Master 5%- unmute"
    m:0x0 + c:122
    XF86AudioLowerVolume

"amixer sset Master 0% unmute"
    m:0x0 + c:121
    XF86AudioMute
